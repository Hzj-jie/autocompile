
# this make file is generated by autocompile,
# with .autocompile configuration file,
# so change the configuration file or autocompile logic is prefered

all: \
 a.h.pch \
 b.hpp.pch \
 aa.o \
 a.o \
 main

a.h.pch: a.h
	g++ -x c++-header a.h -o a.h.pch -std=c++11 -Wall -Wno-unused-function -Wno-unused-variable -O3

b.hpp.pch: b.hpp
	g++ -x c++-header b.hpp -o b.hpp.pch -std=c++11 -Wall -Wno-unused-function -Wno-unused-variable -O3

aa.o: aa.cpp
	g++ -c aa.cpp -o aa.o -std=c++11 -Wall -Wno-unused-function -Wno-unused-variable -O3

a.o: a.cpp
	g++ -c a.cpp -o a.o -std=c++11 -Wall -Wno-unused-function -Wno-unused-variable -O3

main: main.cpp a.h b.hpp \
 a.h.pch \
 b.hpp.pch \
 aa.o \
 a.o
	g++ main.cpp -o main -std=c++11 -Wall -Wno-unused-function -Wno-unused-variable -O3 -lboost_system aa.o a.o

clean:
	-rm a.h.pch b.hpp.pch aa.o a.o main

autocompile:
	autocompile > Makefile

