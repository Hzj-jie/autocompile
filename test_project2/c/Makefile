
# this make file is generated by autocompile,
# with .autocompile configuration file,
# so change the configuration file or autocompile logic is prefered

all: \
 c.h.pch \
 c.o

c.h.pch: c.h
	g++ -x c++-header c.h -o c.h.pch -std=c++11 -Wall -Wno-unused-function -Wno-unused-variable -O3

c.o: c.cpp c.h
	g++ -c c.cpp -o c.o -std=c++11 -Wall -Wno-unused-function -Wno-unused-variable -O3

clean:
	-rm c.h.pch c.o

autocompile:
	autocompile > Makefile

